{
    "compilerOptions": {
      "target": "ES3",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strict": true,
      "sourceMap": true
    }
}
